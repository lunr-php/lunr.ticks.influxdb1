{
  "name": "lunr/ticks.influxdb1",
  "description": "Lunr.Ticks.InfluxDB1 is a library for profiling and event logging with the InfluxDB 1.x API",
  "keywords": [ "profiler", "profiling", "event logging" ],
  "homepage": "https://github.com/lunr-php/lunr.ticks.influxdb1",
  "type": "library",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/move-backend/influxdb-php.git"
    }
  ],
  "require": {
    "php": ">=8.1",
    "lunr/halo": "dev-master",
    "lunr/ticks": "dev-master",
    "psr/log": ">=1.1",
    "influxdb/influxdb-php": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.0 <9.6",
    "ext-xdebug": "~3.1"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
